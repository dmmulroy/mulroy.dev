{
  "name": "mulroy-dev",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build:registry": "pnpm --filter @mulroy-dev/registry build",
    "deploy": "pnpm build:registry && pnpm --filter @mulroy-dev/registry run deploy && pnpm --filter @mulroy-dev/main run deploy",
    "deploy:main": "pnpm --filter @mulroy-dev/main run deploy",
    "deploy:registry": "pnpm build:registry && pnpm --filter @mulroy-dev/registry run deploy",
    "dev": "pnpm --filter @mulroy-dev/main run dev",
    "dev:registry": "pnpm --filter @mulroy-dev/registry run dev",
    "test": "pnpm -r test",
    "typecheck": "pnpm -r typecheck"
  },
  "devDependencies": {
    "typescript": "^5.5.2",
    "vitest": "^2.1.0",
    "wrangler": "^4.51.0"
  }
}
