{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "mulroy-dev-registry",
  "main": "src/index.tsx",
  "compatibility_date": "2025-11-27",
  "assets": {
    "directory": "./public",
    "binding": "ASSETS"
  },
  "observability": {
    "logs": {
      "enabled": true,
      "head_sampling_rate": 1,
      "persist": true,
      "invocation_logs": true
    },
    "traces": {
      "enabled": true,
      "head_sampling_rate": 1,
      "persist": true
    }
  }
}
